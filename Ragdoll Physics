local SolarisLib = loadstring(game:HttpGet("https://solarishub.dev/SolarisLib.lua"))()


local win = SolarisLib:New({
   Name = "SolarisLib",
   FolderToSave = "SolarisLibStuff"
})

local tab = win:Tab("Main")

local sec = tab:Section("Main")
----------------------------------------------

----------------------------------------------
sec:Button("Server Browser", function()
   SolarisLib:Notification("Notice!!", "Server browser is not my script look on credit ")
loadstring(game:HttpGet("https://www.scriptblox.com/raw/Server-Browser_80", true))();

end)

local slider = sec:Slider("Points", 0,1000000,0,2.5,"Slider", function(t)
for i = 1,(t) do --change 200 to how many times do you want
    -- This script was generated by Hydroxide's RemoteSpy: https://github.com/Upbolt/Hydroxide
 
local ohInstance1 = game.Players.LocalPlayer.Character.Torso
local ohString2 = "29312939129391931HCDIHCNDSjincjisnedcisd"
 
game:GetService("ReplicatedStorage").Impact:FireServer(ohInstance1, ohString2)
end
end)
-------------------------------------------
local sec = tab:Section("Fe")
sec:Textbox("Music id here ----------------------------------------->", true, function(id)

local ohNumber1 = 500000000 --Maxdistance
local ohBoolean2 = true --looped
local ohNumber3 = 10 --emitter thing idk
local ohNumber4 = 5 --volume
local ohString5 = "http://www.roblox.com/asset/?id="..id
local ohNumber6 = 0 --animation
 
game:GetService("ReplicatedStorage").EmoteSound:FireServer(ohNumber1, ohBoolean2, ohNumber3, ohNumber4, ohString5, ohNumber6)end)

sec:Button("Stop music", function()
   SolarisLib:Notification("Stopped", "This only stopped music that you playing")
 game:GetService("ReplicatedStorage").EmoteSoundStop:FireServer()
end)

sec:Button("Btools", function()
   SolarisLib:Notification("Btools", "Epic Btools loaded :troll:")
local Lighting = game:GetService("Lighting");
local ReplicatedStorage = game:GetService("ReplicatedStorage");
local Players = game:GetService("Players");
local Remote = ReplicatedStorage:FindFirstChild("DestroyObj");

local Tool = Instance.new("Tool", Players.LocalPlayer.Backpack);
Tool.RequiresHandle = false
Tool.Name = "Delete"
Tool.Activated:Connect(function()
Remote:FireServer(Players.LocalPlayer:GetMouse().Target);
wait()
if Players.LocalPlayer:GetMouse().Target.Transparency == 1  then
Players.LocalPlayer:GetMouse().Target.Parent = Lighting;
end
end)
end)

sec:Textbox("Epic Message", true, function(t)
game.ReplicatedStorage.GiveBadge:FireServer(1, (t) ); --1st number is badge id 2nd is badge name
end)



local tab2 = win:Tab("trolling")

local sec = tab2:Section("trolling")





local tab3 = win:Tab("Player")

local sec3 = tab2:Section("Player")

local slider = sec:Slider("Walkspeed", 0,500,16,1,"Slider", function(t)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = t
end)




